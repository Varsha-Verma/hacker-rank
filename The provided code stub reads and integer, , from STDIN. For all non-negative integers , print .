n =int(input())
[print(i**2) for i in range(n) if n in range(1, 21)]  #for constraint n should be between 1 and 20

#without constraint
[print(i**2) for i in range(n)]   


#another method:
n =int(input())
counter = 0;
    while  counter < n :
        print (counter**2)
        counter = counter + 1
